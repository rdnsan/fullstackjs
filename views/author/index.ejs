<h2>Search Author</h2>
<form action="/author" method="GET">
  <label for="name">Name</label>
  <input type="text" name="name" id="name" value="<%= searchOptions.name %>">
  <button type="submit">Search</button>
</form>
<br>
<% authors.forEach((author) => { %>
  <div><%= author.name %></div>
<% }) %>